{
    "compileOnSave": false,
    "compilerOptions": {
        "moduleResolution": "node",
        "target": "ESNext",
        "module": "ESNext",
        "lib": [
            "ESNext"
        ],
        "outDir": "./dist",
        "strict": true,
        "sourceMap": true,
        "declaration": false,
        "downlevelIteration": true,
        "importHelpers": true,
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "./main.ts"
    ]
}